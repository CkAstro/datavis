{
   "name": "client",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "start": "webpack serve",
      "release": "npm run lint && npm run format:check && npm run ts-check && npm run spellcheck && npm run test && npm run build",
      "build": "npm run build:production",
      "build:production": "npm run clean:dist && webpack --node-env production",
      "build:development": "npm run clean:dist && webpack --node-env development",
      "bundle-analysis": "webpack --node-env production --env showBundleAnalysis",
      "lint": "eslint . --max-warnings 0",
      "lint:fix": "npm run lint -- --fix",
      "test": "jest --clearCache && jest --watchAll",
      "ts-check": "tsc --skipLibCheck",
      "format": "prettier . --write --cache --loglevel warn",
      "format:check": "prettier . --check --cache --loglevel warn",
      "spellcheck": "cspell --no-progress --dot \"**/*.{js,ts,jsx,tsx,json,html,xml,svg,css,scss,sass,md}\"",
      "list-outdated-dependencies": "npm-check-updates --format repo,group --peer",
      "update-dependencies": "npm run list-outdated-dependencies -- -u && npm install && npm update && npm run lint:fix && npm run format && npm run release",
      "clean": "rm -rf node_modules coverage dist",
      "clean:dist": "rm -rf dist",
      "prepare": "husky install"
   },
   "eslintConfig": {
      "extends": [
         "react-app",
         "react-app/jest"
      ]
   },
   "dependencies": {
      "axios": "^0.27.2",
      "gl-matrix": "^3.4.3",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-scripts": "5.0.1",
      "universal-cookie": "^4.0.4"
   },
   "devDependencies": {
      "@babel/core": "^7.18.10",
      "@babel/eslint-parser": "^7.18.9",
      "@babel/preset-env": "^7.18.10",
      "@babel/preset-react": "^7.18.6",
      "@babel/preset-typescript": "^7.18.6",
      "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
      "@testing-library/jest-dom": "^5.16.5",
      "@testing-library/react": "^13.3.0",
      "@testing-library/user-event": "^14.4.3",
      "@types/jest": "^28.1.7",
      "@types/react": "^18.0.17",
      "@types/react-dom": "^18.0.6",
      "@types/react-router-dom": "^5.3.3",
      "@typescript-eslint/eslint-plugin": "^5.33.1",
      "@typescript-eslint/parser": "^5.33.1",
      "@types/axios": "^0.14.0",
      "babel-loader": "^8.2.5",
      "copy-webpack-plugin": "^11.0.0",
      "core-js": "^3.24.1",
      "cspell": "^6.8.0",
      "css-loader": "^6.7.1",
      "css-minimizer-webpack-plugin": "^4.0.0",
      "eslint": "^8.22.0",
      "eslint-config-airbnb": "^19.0.4",
      "eslint-config-prettier": "^8.5.0",
      "eslint-plugin-import": "^2.26.0",
      "eslint-plugin-jsx-a11y": "^6.6.1",
      "eslint-plugin-prettier": "^4.2.1",
      "eslint-plugin-react": "^7.30.1",
      "eslint-plugin-react-hooks": "^4.6.0",
      "fork-ts-checker-webpack-plugin": "^7.2.13",
      "html-minimizer-webpack-plugin": "^4.1.0",
      "html-webpack-plugin": "^5.5.0",
      "husky": "^8.0.1",
      "identity-obj-proxy": "^3.0.0",
      "jest": "^28.1.3",
      "jest-environment-jsdom": "^28.1.3",
      "lint-staged": "^13.0.3",
      "mini-css-extract-plugin": "^2.6.1",
      "mini-svg-data-uri": "^1.4.4",
      "npm-check-updates": "^16.0.5",
      "postcss": "^8.4.16",
      "postcss-loader": "^7.0.1",
      "postcss-preset-env": "^7.8.0",
      "prettier": "^2.7.1",
      "react-refresh": "^0.14.0",
      "sass": "^1.54.5",
      "sass-loader": "^13.0.2",
      "style-loader": "^3.3.1",
      "typescript": "^4.7.4",
      "webpack": "^5.74.0",
      "webpack-bundle-analyzer": "^4.6.0",
      "webpack-cli": "^4.10.0",
      "webpack-dev-server": "^4.10.0"
   },
   "engines": {
       "node": ">=16.8.0"
   }
}
